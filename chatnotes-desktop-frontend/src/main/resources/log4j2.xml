<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="error">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout>
                <MarkerPatternSelector defaultPattern="%d{yyyy-MM-dd HH:mm:ss.SSS} %-5p %c{1.}: %m%n">
                    <PatternMatch key="CLASS" pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} %-5p %c{1.}: %m%n"/>
                </MarkerPatternSelector>
            </PatternLayout>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="TRACE">
            <AppenderRef ref="Console" />
        </Root>
    </Loggers>
</Configuration>